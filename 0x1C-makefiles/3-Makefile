CC = gcc
SRS = main.c school.c
OBJ = school.o main.o
NAME = school
RM = rm -f

all : $(OBJ)
	$(CC) $(OBJ) -o $(NAME)

main.o : main.c
	$(CC)	-c -o main.o main.c

school.o : school.c
	$(CC)	-c -o school.o school.c

clean :
	$(RM) *~ $(NAME)

oclean :
	$(RM) $(OBJ)

fclean :
	$(RM) *~ $(NAME)
	$(RM) $(OBJ)

re : oclean all
